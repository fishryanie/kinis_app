{
  "name": "Kinis",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "reload": "rm -rf node_modules yarn.lock && cd ios && rm -rf pods podfile.lock && cd .. && yarn && yarn pod-install:apple-silicon",
    "pod-install:apple-silicon": "cd ios && arch -x86_64 pod install && cd ..",
    "android-debug": "adb reverse tcp:9090 tcp:9090",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "build-apk": "cd android && rm -rf .gradle && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "install-apk": "cd android && rm -rf .gradle && ./gradlew clean && ./gradlew installRelease && cd ..",
    "build-bundle": "cd android && rm -rf .gradle && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "archive": "cd ios && agvtool new-version -all $(date +%s) && xcodebuild -workspace Kinis.xcworkspace -scheme Kinis clean archive -configuration release -sdk iphoneos && cd .."
  },
